{
  "name": "backends",
  "version": "1.0.0",
  "main": "index.js",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "build": "tsc",
    "start": "node dist/index.js",
    "dev": "nodemon --watch src --exec ts-node src/index.ts",
    "psql::generate": "npx prisma generate --schema prisma/schema.prisma",
    "psql::migrate": "npx prisma migrate dev --schema prisma/schema.prisma",
    "psql::studio": "npx prisma studio --schema prisma/schema.prisma",
    "mysql::generate": "npx prisma generate --schema prisma-mysql/schema.prisma",
    "mysql::studio": "npx prisma studio --schema prisma-mysql/schema.prisma",
    "mysql::dbpush": "npx prisma db push --schema prisma-mysql/schema.prisma",
    "mysql::dbpull": "npx prisma db pull --schema prisma-mysql/schema.prisma",
    "mysql::migrate": "npx prisma migrate dev --schema prisma-mysql/schema.prisma"
  },
  "prisma": {
    "seed": "ts-node prisma/seed.ts"
  },
  "keywords": [],
